<div class="panel panel-primary">
    <div class="panel-heading">
        <i class="search icon"></i>GANG CONTAINER
    </div>
    <div class="panel-body">

        <table class="table table-stripped table-bordered table-condensed table-hover">
            <tr>
                <th>SN</th>
                <th>CONTAINER/VESSEL</th>
                <th>NUMBER</th>
            </tr>
            <tbody>
                    @foreach (var item in ViewBag.VList as List<GetLabourManager.ViewModel.VesselContainerViewModel>)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.Container</td>
                            <td>@item.Number</td>
                        </tr>
                    }
            </tbody>
        </table>
        <div class="row">
            <div class="col-md-6">

            </div>
            <div class=" col-md-6">
                <div class="pull-right">
                    <button id="btnOK" class="btn btn-primary btn-lg">
                        OK
                    </button>
                </div>
             
            </div>
        </div>
    </div>
</div>
<script>
    $(function () {
        $('#btnOK').on('click', function () {
            $('.ui.basic.modal').modal('hide');
        });
    })
</script>